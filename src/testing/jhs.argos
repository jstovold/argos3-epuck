<?xml version="1.0" ?>
<argos-configuration>

    <framework>
        <experiment length="5000"
                    ticks_per_second="10"/>
    </framework>

    <controllers>
        <test_controller id="jhs"
                         library="build_sim/testing/libtest_controller.so">

            <sensors>
            </sensors>

            <actuators>
                <epuck_wheels implementation="default"/>
                <epuck_base_leds implementation="default" medium="leds"/>
                <epuck_rgb_leds implementation="default" medium="leds"/>
            </actuators>

            <params/>
        </test_controller>
    </controllers>

    <arena size="5,5,1" center="0,0,0.5">
        <e-puck id="ep0">
            <body position="0,0,0" orientation="0,0,0"/>
            <controller config="test_controller" />
        </e-puck>
    </arena>

    <physics_engines>
        <dynamics2d id="dyn2d" />
    </physics_engines>

    <media>
        <led id="leds"/>
    </media>

    <visualization>
        <qt-opengl/>
    </visualization>

</argos-configuration>
