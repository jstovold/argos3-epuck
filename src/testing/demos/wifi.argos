<?xml version="1.0" ?>
<argos-configuration>

    <framework>
        <experiment length="5000"
                    ticks_per_second="8"/>
    </framework>

    <controllers>
        <demo_wifi_controller id="demo_wifi"
                         library="build_sim/testing/demos/libwifi.so">

            <sensors>
		<epuck_wifi implementation="default"/>
            </sensors>

            <actuators>
                <epuck_wifi implementation="default"/>
            </actuators>

            <params/>
        </demo_wifi_controller>
    </controllers>

    <arena size="5,5,1" center="0,0,0.5">
        <e-puck id="ep0">
            <body position="0,0,0" orientation="0,0,0"/>
            <controller config="demo_wifi" />
        </e-puck>
    </arena>

    <physics_engines>
        <dynamics2d id="dyn2d" />
    </physics_engines>

    <media>
    </media>

    <visualization>
        <qt-opengl/>
    </visualization>

</argos-configuration>
